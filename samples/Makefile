ifndef config
  config=debug
endif
export config

PROJECTS := testIRandomX ex-ran

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

testIRandomX: 
	@echo "==== Building testIRandomX ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f testIRandomX.make

ex-ran: 
	@echo "==== Building ex-ran ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f ex-ran.make

clean:
	@${MAKE} --no-print-directory -C . -f testIRandomX.make clean
	@${MAKE} --no-print-directory -C . -f ex-ran.make clean

help:
